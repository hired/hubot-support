#!/usr/bin/env coffee

server = (require 'http').createServer (req, res) ->
primus = new (require 'primus') server,
  transformer: 'browserchannel'
  parser: 'json'

console.log """
// The client library for Primus
// Generated by bin/primus-client-js at #{Date.now()}
module = {exports: {}};
#{primus.library()}
"""
